{
  "name": "eslint-plugin-angular-whitespace",
  "version": "14.0.1",
  "description": "ESLint plugin to control whitespace in angular templates",
  "repository": {
    "type": "git",
    "url": "https://github.com/FDIM/eslint-plugin-angular-whitespace"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "whitespace",
    "angular",
    "template"
  ],
  "author": "FDIM",
  "main": "dist/index.js",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --project ./src/tsconfig.json ",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "validate": "npm run lint && npm run test"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "@angular-eslint/bundled-angular-compiler": "14.1.2",
    "@angular-eslint/utils": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@typescript-eslint/parser": "5.53.0",
    "@typescript-eslint/eslint-plugin": "5.53.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run validate"
    }
  },
  "license": "ISC"
}
